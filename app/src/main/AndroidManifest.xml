        <activity android:name="com.balckhao.testActivities.DownloadProgressButtonActivity"/>
        <activity android:name=".radioGroup.RelativeRadioGroupActivity" />
        <activity android:name="com.balckhao.testActivities.spinnerNew.TestSpinnerNewActivity" />
